<v-row>\n        <v-col cols=\"12\"\n          ><v-card-title style=\"font-size:20px\">\n            การรายงานปัญหา\n          </v-card-title></v-col\n        ></v-row\n      >\n      <v-row>\n        <v-container fluid>\n          <v-row>\n            <v-col cols=\"6\">\n              <v-card outlined class=\"pa-3\">\n                <v-row class=\"align-center\">\n                  <v-col\n                    ><pie-chart\n                      :colors=\"['#C62828', '#FFA726']\"\n                      :data=\"pieData\"\n                      :legend=\"false\"\n                    ></pie-chart\n                  ></v-col>\n                  <v-col>\n                    <p>\n                      <v-icon style=\"color: #FFA726\" small>mdi-circle</v-icon>\n                      ชำระผ่านพร้อมเพย์\n                    </p>\n                    <p>{{ elecPercent() }} %</p>\n                    <p>\n                      <v-icon style=\"color: #C62828\" small>mdi-circle</v-icon>\n                      ชำระผ่านบัตรเครดิต\n                    </p>\n                    <p>{{ appPercent() }} %</p>\n                  </v-col>\n                </v-row>\n              </v-card>\n            </v-col>\n            <v-col cols=\"6\">\n              <v-card\n                class=\"align-center justify-center pa-3\"\n                height=\"100%\"\n                outlined\n              >\n                <v-container fluid fill-height>\n                  <v-row>\n                    <v-col cols=\"12\">\n                      <v-card outlined height=\"100%\">\n                        <v-card-title style=\"font-size:1em\">\n                          การรายงานปัญหาทั้งหมด\n                        </v-card-title>\n                        <v-card-text> {{ allReport() }} </v-card-text>\n                      </v-card>\n                    </v-col>\n                  </v-row>\n\n                  <v-row>\n                    <v-col cols=\"6\" height=\"100%\">\n                      <v-card outlined style=\"border-color:#C62828\">\n                        <v-card-title style=\"font-size:1em\">\n                          ปัญหาจากแอพพลิเคชั่น\n                        </v-card-title>\n                        <v-card-text> {{ pieData[0][1] }} </v-card-text>\n                      </v-card>\n                    </v-col>\n                    <v-col cols=\"6\" height=\"100%\">\n                      <v-card outlined style=\"border-color:#FFA726\">\n                        <v-card-title style=\"font-size:1em\">\n                          ปัญหาจากอุปกรณ์\n                        </v-card-title>\n                        <v-card-text> {{ pieData[1][1] }} </v-card-text>\n                      </v-card>\n                    </v-col>\n                  </v-row>\n                </v-container>\n              </v-card>\n            </v-col>\n          </v-row>\n        </v-container>\n      </v-row>\n    </v-container>\n  </v-card>\n</template>\n\n<script>\nexport default {\n  data: function() {\n    return {\n      pieData: [\n        [\"แอพพลิเคชั่น\", 4],\n        [\"อุปกรณ์\", 1]\n      ]\n    };\n  },\n\n  methods: {\n    appPercent: function() {\n      return (\n        (this.pieData[0][1] / (this.pieData[0][1] + this.pieData[1][1])) *\n        100\n      ).toFixed(2);\n    },\n\n    elecPercent: function() {\n      return (100 - this.appPercent()).toFixed(2);\n    },\n\n    allReport: function() {\n      return this.pieData[0][1] + this.pieData[1][1];\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/components"}]}